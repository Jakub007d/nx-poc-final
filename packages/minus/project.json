{
  "name": "@jakub007d/minus",
  
  "sourceRoot": "packages/minus/src",
  "projectType": "library",
  "tags": ["0.0.1"],
  
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/minus",
        "main": "packages/minus/src/index.ts",
        "tsConfig": "packages/minus/tsconfig.lib.json",
        "assets": ["packages/minus/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["LINT_PATTERNS"]
      }
    }
  },
  "implicitDependencies": [],
  "dependencies": []
}